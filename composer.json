{
  "name": "ondrej7/webloader",
  "description": "Tool for loading or deploying CSS and JS files into web pages",
  "keywords": [
    "webloader",
    "css",
    "javascript",
    "assets",
    "nette"
  ],
  "homepage": "http://addons.nette.org/cs/webloader",
  "license": "MIT",
  "authors": [
    {
      "name": "Jan Marek",
      "email": "mail@janmarek.net"
    },
    {
      "name": "tomasvinduska"
    }
  ],
  "replace": {
    "janmarek/webloader": "*"
  },
  "autoload": {
    "psr-0": {
      "WebLoader": ""
    }
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/phpcoffee/coffeescript"
    }
  ],
  "require": {
    "php": ">=5.6"    
  },
  "suggest": {
    "oyejorge/less.php": "LESS compiler written in PHP.",
    "leafo/scssphp": "SCSS compiler written in PHP.",
    "joseki/webloader-filters": "CSSMin & JSMin filters written in PHP.",
    "coffeescript/coffeescript": "CoffeeScript compiler written in PHP."
  },
  "scripts": {
    "test": [
      "./vendor/bin/parallel-lint -e php --exclude vendor .",
      "vendor/bin/phpunit --configuration tests/phpunit.xml tests"
    ]
  }
}
